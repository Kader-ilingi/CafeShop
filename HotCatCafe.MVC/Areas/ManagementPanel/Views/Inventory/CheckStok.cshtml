@model HotCatCafe.MVC.Models.ErrorViewModel
@{
    ViewData["Title"] = "CheckStok";
    Layout = "~/Areas/ManagementPanel/Views/Shared/_ManagementPanelLayout.cshtml";
}

<h1>CheckStok</h1>

<h1>An Error Occurred</h1>

<p>
    @Model.ErrorMessage
</p>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> @Model.RequestId
    </p>
}